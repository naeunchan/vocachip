{
    "expo": {
        "name": "vocachip",
        "slug": "vocachip",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1d4ed8"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.vocachip.app",
            "buildNumber": "1.0.0"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.vocachip.app",
            "versionCode": 1
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-audio", "expo-secure-store", "sentry-expo", "expo-asset"],
        "extra": {
            "versionLabel": "1.0.0",
            "featureGuestAccountCta": true,
            "featureBackupRestore": false,
            "privacyPolicyUrl": "https://vocachip.app/legal/privacy",
            "termsOfServiceUrl": "https://vocachip.app/legal/terms",
            "featureProfile": "development",
            "openAIProxyUrl": "",
            "openAIProxyKey": "",
            "aiHealthUrl": "",
            "firebaseApiKey": "",
            "firebaseAuthDomain": "",
            "firebaseProjectId": "",
            "firebaseAppId": "",
            "firebaseMessagingSenderId": "",
            "firebaseStorageBucket": "",
            "firebaseMeasurementId": "",
            "eas": {
                "projectId": "863c5cc0-daf6-4396-a1d0-4ace91755f0a"
            }
        }
    }
}
