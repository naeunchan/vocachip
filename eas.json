{
    "cli": {
        "version": ">= 11.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_ENV": "development"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "APP_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
