{
    "expo": {
        "name": "vocationary",
        "slug": "vocationary",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1d4ed8"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.vocationary.app",
            "buildNumber": "1.0.0"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "package": "com.vocationary.app",
            "versionCode": 1
        },
        "web": {
            "favicon": "./assets/favicon.png"
        },
        "plugins": ["expo-sqlite", "expo-audio", "expo-secure-store", "sentry-expo", "expo-asset"],
        "extra": {
            "versionLabel": "1.0.0",
            "featureGuestAccountCta": false,
            "featureBackupRestore": false,
            "privacyPolicyUrl": "https://vocationary.app/legal/privacy",
            "termsOfServiceUrl": "https://vocationary.app/legal/terms"
        }
    }
}
